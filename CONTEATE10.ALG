Algoritmo "conteate10"

Var
   contador, valor: inteiro



Inicio
      contador <- 0
      EscrevaL ("Quer contar até quanto? ")
      Leia (valor)
      Enquanto (contador <= valor) faca
               EscrevaL (contador)
               contador <- contador + 1
      FimEnquanto
      EscrevaL ("")
      EscrevaL ("Terminei de contar")
   



Fimalgoritmo